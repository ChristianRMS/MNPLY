package entities;

/**
 * created by Christian Zen
 * christian.zen@outlook.de
 * Date of creation: 26.04.2016
 */
public class Player {

	private String id;
	private String name;
	private String uri;
	private boolean isReady;

	public Player(String id, String name, String uri) {
		this.id = id;
		this.name = name;
		this.uri = uri;
	}

	public String getName() {
		return name;
	}

	public String getId() {
		return id;
	}

	public String getUri() {
		return uri;
	}

	public boolean isReady() {
		return isReady;
	}

	public void setIsReady(boolean isReady) {
		this.isReady = isReady;
	}
}
